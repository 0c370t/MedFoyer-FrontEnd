server {
    listen   80;
    listen   [::]:80 default ipv6only=on;

    root /usr/share/nginx/html;
    index index.html;

    server_name _; # all hostnames


    location /patient/ {
        try_files $uri /patient/index.html;
    }

    location /clinic/ {
        try_files $uri /clinic/index.html;
    }

    location / {
        try_files $uri /index.html;
    }
}